import pandas as pd

# Sample: df is your DataFrame
# Replace 'T' with space and convert to datetime with UTC offset handled
df['created'] = df['created'].str.replace('T', ' ', regex=False)
df['created'] = pd.to_datetime(df['created'], errors='coerce', utc=True)

# Optionally convert to string format BigQuery likes (e.g. 'YYYY-MM-DD HH:MM:SS')
df['created'] = df['created'].dt.strftime('%Y-%m-%d %H:%M:%S')