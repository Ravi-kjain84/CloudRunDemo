SELECT cl.jira_id,
       cl.change_value AS latest_status,
       cl.timestamp
FROM change_log cl
JOIN (
    SELECT jira_id, MAX(timestamp) AS max_timestamp
    FROM change_log
    WHERE change_type = 'Status'
    GROUP BY jira_id
) latest
ON cl.jira_id = latest.jira_id AND cl.timestamp = latest.max_timestamp
WHERE cl.change_type = 'Status';