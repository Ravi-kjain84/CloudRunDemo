SELECT 
  Jira_Id,
  issue_type,
  Requirement_Id,
  label,
  Filter_Label,
  Epic,
  'Design Time' AS Phase,
  Design_Time_days AS Duration_days
FROM hsbc-9093058-rwapc52-dev.log_ds.RJ_jira_table_new

UNION ALL

