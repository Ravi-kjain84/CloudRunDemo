SELECT *,
  TRIM(SUBSTR(filter_label, STRPOS(filter_label, ":") + 1)) AS release_label,
  CASE
    WHEN Timestamp = latest_status_ts THEN 'Yes'
    ELSE 'No'
  END AS latest_status_flag
FROM (
  SELECT *,
    MAX(CASE WHEN Change_Item = 'status' THEN Timestamp END) 
      OVER (PARTITION BY Jira_Id ORDER BY Timestamp DESC 
            ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) AS latest_status_ts
  FROM `hsbc-9093058-rwapc52-dev.log_ds.RJ_jira_logs`
)