def _calculate_raw_total_time(self, group: pd.DataFrame) -> Optional[float]:
    """
    Compute raw time difference between earliest and latest timestamp in the group.
    """
    if group['Timestamp'].isnull().all():
        return None
    min_ts = group['Timestamp'].min()
    max_ts = group['Timestamp'].max()
    duration = (max_ts - min_ts).total_seconds()
    return round(duration / 86400, 2) if duration > 0 else None