for _, row in matching_rows.iterrows():
    ws.append([
        row.get("Step Number", ""),
        row.get("Test Step", ""),
        row.get("Expected Result", ""),
        row.get("Result", "")
    ])
    
    current_row = ws.max_row
    for col in range(1, 5):
        cell = ws.cell(row=current_row, column=col)
        cell.alignment = Alignment(wrap_text=True)